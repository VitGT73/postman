									"});\r",
									""
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Выполнение запроса\r",
									"pm.sendRequest({\r",
									"    url: 'https://restful-booker.herokuapp.com/booking',\r",
									"    method: 'GET',\r",
									"    header: {\r",
									"        'Content-Type': 'application/json',\r",
									"    },\r",
									"}, function (err, response) {\r",
									"    // Обработка ответа\r",
									"    if (err) {\r",
									"        console.error('Ошибка при выполнении запроса:', err);\r",
									"    } else {\r",
									"        // Парсим JSON-ответ\r",
									"        var jsonData = response.json();\r",
									"\r",
									"        // Проверяем, есть ли бронирования в ответе\r",
									"        if (jsonData.length > 0) {\r",
									"            // Выбираем случайное бронирование\r",
									"            var randomBooking = jsonData[Math.floor(Math.random() * jsonData.length)];\r",
									"\r",
									"            // Получаем значение bookingid\r",
									"            var bookingId = randomBooking.bookingid;\r",
									"\r",
									"            // Выводим результат в консоль\r",
									"            // console.log(\"Случайный bookingid: \", bookingId);\r",
									"\r",
									"            // Сохраняем значение в переменную коллекции\r",
									"            pm.collectionVariables.set(\"BookingID\", bookingId);\r",
									"        } else {\r",
									"            console.error('В ответе нет BookingID');\r",
									"        }\r",
									"    }\r",
									"});\r",
									""
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{URL}}/booking/{{BookingID}}",
							"host": [
